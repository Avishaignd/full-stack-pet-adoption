e>{props.pet.name}</Card.Title>\r\n            <Card.Text>Pet Description:</Card.Text>\r\n            <ListGroup>\r\n              <ListGroupItem>Height: {props.pet.height} </ListGroupItem>\r\n              <ListGroupItem>Weight: {props.pet.weight} </ListGroupItem>\r\n              <ListGroupItem>Type: {props.pet.type} </ListGroupItem>\r\n              <ListGroupItem>Color: {props.pet.color}</ListGroupItem>\r\n              <ListGroupItem>\r\n                Hypoallergenic: {props.pet.hypoallergenic ? \"Yes\" : \"No\"}\r\n              </ListGroupItem>\r\n              <ListGroupItem>\r\n                Dietary Restrictions: {props.pet.dietaryRestrictions}\r\n              </ListGroupItem>\r\n              <ListGroupItem>Breed: {props.pet.breed}</ListGroupItem>\r\n            </ListGroup>\r\n          </Card.Body>\r\n        </Card>\r\n      </div>\r\n    \r\n  );\r\n}\r\n","C:\\Users\\user\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\components\\Search.js",["187","188","189","190","191"],"C:\\Users\\user\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\components\\api.js",[],"C:\\Users\\user\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\components\\AdminPage.js",[],"C:\\Users\\user\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\components\\AddPet.js",[],"C:\\Users\\user\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\components\\AdminDashboard.js",[],"C:\\Users\\USER\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\index.js",[],["192","193"],"C:\\Users\\USER\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\App.js",["194","195"],"import \"./App.css\";\r\nimport React from 'react'\r\nimport {useEffect, useState} from 'react'\r\nimport { HomePage } from \"./components/HomePage\";\r\nimport { myContext } from \"./components/Context\";\r\nimport { getData, setTokens } from \"./components/api\";\r\nfunction App() {\r\n\r\n  const [allUsers, setAllUsers] = useState([])\r\n  const [allPets, setAllPets] = useState([])\r\n  const [currentUser, setCurrentUser] = useState(undefined)\r\n\r\n  if (localStorage.getItem('user') && !currentUser) {\r\n    setCurrentUser(JSON.parse(localStorage.getItem('user')));\r\n  }\r\n\r\n  if (allPets.length > 0 && currentUser === undefined){\r\n    setAllPets([])\r\n    setAllUsers([])\r\n  }\r\n\r\n    useEffect(() => {\r\n    if (currentUser !== undefined){\r\n      setInterval(() => {\r\n        setTokens('http://localhost:5000/api/users/token', {user: currentUser.email, token: localStorage.getItem('refreshToken')})\r\n      }, 1000 * 60 * 5)\r\n    }\r\n  }, [])\r\n\r\n\r\n  useEffect(() => {\r\n    getData('http://localhost:5000/api/users')\r\n    .then(response => {\r\n      if (currentUser && currentUser.isAdmin){\r\n        setAllUsers(response)\r\n      }\r\n    })\r\n    getData('http://localhost:5000/api/pets')\r\n    .then(response => {\r\n      setAllPets(response)\r\n    })\r\n  },[])\r\n\r\n  \r\n\r\n  return (\r\n    <div className=\"App\">\r\n      <myContext.Provider value={{users: allUsers, pets: allPets, currentUser: currentUser}}>\r\n        <HomePage />\r\n      </myContext.Provider>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\USER\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\reportWebVitals.js",[],"C:\\Users\\USER\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\components\\HomePage.js",["196","197","198","199","200"],"C:\\Users\\USER\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\components\\Context.js",[],"C:\\Users\\USER\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\components\\SignUp.js",[],"C:\\Users\\USER\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\components\\Login.js",[],"C:\\Users\\USER\\Desktop\\pet-adoption-front-end-Avishaignd\\src\\components\\Search.js",["201","202","203"],"import React, { useContext, useState } from \"react\";\r\nimport {Form, Button, ToggleButtonGroup, ToggleButton, FormGroup, Collapse,} from \"react-bootstrap\";\r\nimport { myContext } from \"./Context\";\r\nimport LogOutButton from \"./LogOutButton\";\r\nimport PetCard from \"./PetCard\";\r\nimport SearchResults from \"./SearchResults\";\r\n\r\n\r\nexport default function Search() {\r\n  const [dogsOrCats, setDogsOrCats] = useState(\"dogs\");\r\n  const [searchText, setSearchText] = useState({\r\n    name: \"\",\r\n    weight: 0,\r\n    height: 0,\r\n    petStatus: \"\",\r\n    type: \"\",\r\n  });\r\n  const [isOpen, setIsOpen] = useState(false);\r\n  const [searchedPets, setSearchedPets] = useState([]);\r\n  const {pets} = useContext(myContext);\r\n\r\n  function handleDogs() {\r\n    setDogsOrCats(\"dogs\");\r\n  }\r\n  function handleCats() {\r\n    setDogsOrCats(\"cats\");\r\n  }\r\n\r\n  const onSearchSubmit = (event) => {\r\n    event.preventDefault();\r\n    let petArray = [];\r\n    pets.map((pet) => {\r\n      for (const key in searchText) {\r\n        for (const prop in pet) {\r\n          if (key === prop && searchText[key] === pet[prop]) {\r\n            if (!petArray.includes(pet)){\r\n              petArray.push(pet);\r\n            }\r\n          }\r\n        }\r\n      }\r\n    });\r\n    setSearchedPets(petArray);\r\n\r\n    setSearchText({ name: \"\", weight: 0, height: 0, petStatus: \"\", type: \"\" });\r\n  };\r\n\r\n  return (\r\n    <div>\r\n      <LogOutButton />\r\n      <ToggleButtonGroup className=\"mt-5 d-flex\" type=\"radio\" name=\"lists\">\r\n        <ToggleButton value={1} onClick={handleDogs} className=\"flex-grow-0\">\r\n          Dogs\r\n        </ToggleButton>\r\n        <ToggleButton value={2} onClick={handleCats} className=\"flex-grow-0\">\r\n          Cats\r\n        </ToggleButton>\r\n        <Button\r\n          className=\"flex-grow-0\"\r\n          onClick={() => setIsOpen(!isOpen)}\r\n          variant=\"secondary\"\r\n          aria-controls=\"example-collapse-text\"\r\n          aria-expanded={isOpen}\r\n        >\r\n          Advanced Search\r\n        </Button>\r\n      </ToggleButtonGroup>\r\n      <Collapse in={isOpen}>\r\n        <Form onSubmit={